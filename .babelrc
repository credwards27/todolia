{
    "presets": [ "env" ],
    "plugins": [
        "transform-class-properties",
        [ "transform-es2015-arrow-functions", {
            "spec": true
        } ],
        [ "babel-plugin-transform-builtin-extend", {
            "globals": [ "Error" ]
        } ],
        [ "babel-plugin-root-import", {
            "rootPathSuffix": "src/js"
        } ]
    ]
}
